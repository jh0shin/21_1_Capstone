// none, original
parlai train_model -t personachat:none -m projects.personachat.kvmemnn.kvmemnn:KvmemnnAgent -ltim 60 -vme 10000 -vtim 900 --hops 1 --lins 0 -vp -1 -vmt accuracy -ttim 3600 -shareEmb True -bs 1 -lr 0.1 -esz 500 --margin 0.1 --tfidf False --numthreads 10 -mf "/home/junho/PDA/model/kvmemnn/none" 

// none, revised
parlai train_model -t personachat:none_revised -m projects.personachat.kvmemnn.kvmemnn:KvmemnnAgent -ltim 60 -vme 10000 -vtim 900 --hops 1 --lins 0 -vp -1 -vmt accuracy -ttim 3600 -shareEmb True -bs 1 -lr 0.1 -esz 500 --margin 0.1 --tfidf False --numthreads 10 -mf "/home/junho/PDA/model/kvmemnn/none_revised" 

